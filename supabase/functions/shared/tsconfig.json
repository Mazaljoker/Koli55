{
  "compilerOptions": {
    "target": "ES2020",
    "module": "ES2020",
    "lib": ["ES2020", "DOM"],
    "moduleResolution": "node",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "types": ["@types/node", "./deno"],
    "baseUrl": ".",
    "paths": {
      "*": ["*"]
    }
  },
  "include": ["./**/*.ts"],
  "exclude": ["node_modules"]
}
